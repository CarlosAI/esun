<div style="padding-left: 150px; padding-top: 80px; width: 650px; ">
	


	  <h1>Nuevo Alumno</h1>
	  <hr>
	  <br>

	    <div class="form-group">
	      <input type="text" class="form-control" id="nombre" placeholder="Nombre" name="nombre">
	    </div>
	    <div class="form-group">
	      <input type="text" class="form-control" id="app1" placeholder="Apellido Paterno" name="app1">
	    </div>
	    <div class="form-group">
	      <input type="text" class="form-control" id="app1" placeholder="Apellido Materno" name="app2">
	    </div>
	    <div class="form-group">
    		<label for="uname">Genero</label>
	    	<select class="custom-select" id="inputGroupSelect01">
		    	<option selected>Masculino</option>
		    	<option value="1">Femenino</option>
			</select>
	    </div>
	    <div class="form-group">
	      <input type="text" class="form-control" id="domicilio" placeholder="Domicilio" name="domicilio">
	    </div>
	    <div class="form-group">
	  		<div class="col" style="width: 50%; float: left;">
	  			<input type="text" class="form-control" id="nombre_padre" placeholder="Nombre del padre" name="nombre1">
	  		</div>
	  		<div class="col" style="width: 50%; float: left;">
	  			<input type="text" class="form-control" id="tel1" placeholder="Telefono" name="tel1">
	  		</div>
